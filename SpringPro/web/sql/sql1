DROP DATABASE IF EXISTS SSH;
CREATE DATABASE SSH DEFAULT CHARACTER SET utf8;
USE SSH ;

##创建用户表
CREATE TABLE t_user (
   user_id   INT AUTO_INCREMENT PRIMARY KEY,
   user_name VARCHAR(30),
   password  VARCHAR(32),
   last_visit datetime,
   last_ip  VARCHAR(23)
)ENGINE=InnoDB;

##插入初始化数据
INSERT INTO t_user (user_name,password) VALUES('admin','123456');
COMMIT;